{
  "name": "typescript-jest-template",
  "version": "1.0.0",
  "description": "Template project for Node, Typescript and Jest",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "test": "jest test",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint 'src/**' --fix",
    "prettier": "prettier --check --config .prettierrc.json 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write --config .prettierrc.json 'src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philjhale/node-typescript-jest-template.git"
  },
  "author": "Phil Hale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philjhale/node-typescript-jest-template/issues"
  },
  "homepage": "https://github.com/philjhale/node-typescript-jest-template#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
